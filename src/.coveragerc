[run]
source=pretalx
branch=True
omit = */migrations/*,*/urls.py,*/tests/*,*/admin.py,pretalx/wsgi.py,pretalx/settings.py

[report]
exclude_lines =
    pragma: no cover
    noqa
    def __repr__
    def __str__
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
